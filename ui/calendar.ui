    <string notr="true">/* üåª Base Window */
QWidget {
    background-color: #f8f1e4;
    font-family: &quot;Verdana&quot;, sans-serif;
    font-size: 14px;
    color: #3e2f1c;
}

/* üå∏ ComboBox Styling */
QComboBox {
    background-color: #e3caa5;
    border: 2px solid #a67c52;
    border-radius: 5px;
    padding: 5px;
}

/* üçÇ GroupBox Title and Border */
QGroupBox {
    border: 2px solid #a67c52;
    border-radius: 10px;
    margin-top: 6px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 3px;
    background-color: #d8ba8c;
    font-weight: bold;
}

/* üåø PushButtons (Calendar Days) */
QPushButton {
    background-color: #e7d5b4;
    border: 2px solid #b1926b;
    border-radius: 5px;
    padding: 5px;
}
QPushButton:hover {
    background-color: #f5e7cc;
}
QPushButton:pressed {
    background-color: #d1b387;
}

/* üåº Labels */
QLabel {
    background: transparent;
    font-weight: bold;
    color: #4e3b24;
}

/* üåü Scroll Area (if any) */
QScrollArea {
    background-color: #f9f4e6;
    border: none;
}
